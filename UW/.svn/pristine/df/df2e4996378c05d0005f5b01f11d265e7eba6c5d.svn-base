
set term pdf
set output 'BBYscan.pdf'

set title 'Radial Mode Transitions of Ytterbium-Barium-Barium Chain'
set xlabel '1762nm Laser Frequency Offset from Carrier(MHz)'
set ylabel 'Shelving Probability'
set xrange [0.65:1.12]
set yrange [0:1]
unset key

filename = 'BBYscanout.txt'
plot filename using ($1-85.05):2:(sqrt(($2+0.001)*(1-$2+0.001)/$3)) index 0 lt 1 with errorbars, \
	filename using ($1-85.05):2:(sqrt(($2+0.001)*(1-$2+0.001)/$3)) index 1 lt 2 with errorbars, \
	filename using 1:2:(sqrt(($2+0.001)*(1-$2+0.001)/$3)) index 2 lt 3 with errorbars, \
	filename using 1:2:(sqrt(($2+0.001)*(1-$2+0.001)/$3)) index 3 lt 4 with errorbars, \
	filename using 1:2:(sqrt(($2+0.001)*(1-$2+0.001)/$3)) index 4 lt 5 with errorbars
