
set term pdf
set output 'scan5byb.pdf'

set title 'Barium-Ytterbium-Barium Configuration'
set xlabel '1762nm Laser Frequency (MHz)'
set ylabel 'Shelving Probability'
set yrange [0:1]
unset key

filename = 'scan5byb.txt'
plot filename using 1:2:(sqrt(($2+0.001)*(1-$2+0.001)/$3)) index 0 lt 1 with errorbars, \
	filename using 1:2:(sqrt(($2+0.001)*(1-$2+0.001)/$3)) index 1 lt 2 with errorbars, \
	filename using 1:2:(sqrt(($2+0.001)*(1-$2+0.001)/$3)) index 2 lt 3 with errorbars
